<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.micro.www.mapper.UserMapper">

    <select id="selectById" parameterType="java.lang.String" resultType="com.micro.common.entity.User">
        select *
        from t_user_define
        where user_id = #{userId}
    </select>

    <select id="listUser" parameterType="java.lang.String" resultType="com.micro.common.entity.User">
        select *
        from t_user_define
        where user_name like '%' || ${userName} ||'%'
    </select>

</mapper>

